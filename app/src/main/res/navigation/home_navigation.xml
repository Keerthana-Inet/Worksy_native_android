<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_navigation"
    app:startDestination="@+id/navigation_menu">
    <fragment
        android:id="@+id/navigation_menu"
        android:name="com.worksy.hr.art.views.fragments.menu.MenuFragment"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu">
         <action
             android:id="@+id/action_menufragment_to_navigation_claimrequest"
             app:destination="@id/navigation_request" />

        <action
            android:id="@+id/action_menufragment_to_navigation_claimapproval"
            app:destination="@id/navigation_approval" />
    </fragment>
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.worksy.hr.art.views.fragments.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">

        <action
            android:id="@+id/action_homefragment_to_travel_request_bottomsheet"
            app:destination="@id/navi_travel_request_bottomsheet" />
    </fragment>
    <fragment
        android:id="@+id/navigation_request"
        android:name="com.worksy.hr.art.views.fragments.claimRequest.ClaimRequestListingFragment"
        android:label="fragment_claim_request_listing"
        tools:layout="@layout/fragment_claim_request_listing">

        <action
            android:id="@+id/action_claim_requestlisting_to_createform_bottomsheet"
            app:destination="@id/navi_createform_bottomsheet" />
    </fragment>
    <fragment
        android:id="@+id/navigation_approval"
        android:name="com.worksy.hr.art.views.fragments.claimApproval.ClaimApprovalListFragment"
        android:label="fragment_claim_approval_list"
        tools:layout="@layout/fragment_claim_approval_list">

        <action
            android:id="@+id/action_claim_approval_listing_to_claim_approval_detail"
            app:destination="@id/navi_claim_approval_detail" />
    </fragment>
    <fragment
        android:id="@+id/navigation_calendar"
        android:name="com.worksy.hr.art.views.fragments.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">

    </fragment>

    <fragment
        android:id="@+id/navi_claim_approval_detail"
        android:name="com.worksy.hr.art.views.fragments.claimApproval.ClaimApprovalDetailFragment"
        android:label="fragment_claim_approval_detail"
        tools:layout="@layout/fragment_claim_approval_detail">

    </fragment>


    <fragment
        android:id="@+id/navigation_progress"
        android:name="com.worksy.hr.art.views.fragments.home.InProgressFragment"
        android:label="fragment_in_progress"
        tools:layout="@layout/fragment_in_progress">
        <action
            android:id="@+id/action_navigation_progress_to_travel_request_bottomsheet"
            app:destination="@id/navi_travel_request_detailpage" />
    </fragment>
    <fragment
        android:id="@+id/navi_claim_request_detail"
        android:name="com.worksy.hr.art.views.fragments.claimRequest.ClaimRequestTabDetailFragment"
        android:label="fragment_claim_request_tab_detail"
        tools:layout="@layout/fragment_claim_request_tab_detail">
        <action
            android:id="@+id/action_navi_claim_request_detail_to_claimrequest_tablayout2"
            app:destination="@id/navi_claimrequest_tablayout2" />
      <!--  <action
            android:id="@+id/action_navi_claim_request_detail_to_claimrequest_tab2"
            app:destination="@id/navi_claimrequest_tab2" />-->
        <!--<action
            android:id="@+id/action_navi_claim_request_detail_to_editreq_bottomsheet"
            app:destination="@id/navi_editreq_bottomsheet" />-->
        <action
            android:id="@+id/action_navi_claim_request_detail_to_custom_bottomsheet"
            app:destination="@id/navi_custom_bottomsheet" />

    </fragment>

    <fragment
        android:id="@+id/navi_custom_bottomsheet"
        android:name="com.worksy.hr.art.views.fragments.claimRequest.CustomBottomSheetDialog"
        android:label="create_claim_form_layout"
        tools:layout="@layout/create_claim_form_layout">
    </fragment>
    <fragment
        android:id="@+id/navi_claimrequest_tablayout2"
        android:name="com.worksy.hr.art.views.fragments.claimRequest.ClaimRequestTabLayout2"
        android:label="fragment_claim_request_tab2"
        tools:layout="@layout/fragment_claim_request_tab_layout2">
    </fragment>
    <fragment
        android:id="@+id/navi_claimrequest_tab2"
        android:name="com.worksy.hr.art.views.fragments.claimRequest.ClaimRequestTab2"
        android:label="fragment_claim_request_tab_layout2"
        tools:layout="@layout/fragment_claim_request_tab2">
    </fragment>
   <!-- <fragment
        android:id="@+id/navi_editreq_bottomsheet"
        android:name="com.worksy.hr.art.views.fragments.claimRequest.EditRequestBottomSheet"
        android:label="fragment_edit_request_bottom_sheet"
        tools:layout="@layout/fragment_edit_request_bottom_sheet"/>-->
    <fragment
        android:id="@+id/navi_travel_request_bottomsheet"
        android:name="com.worksy.hr.art.views.fragments.travelRequest.TravelRequestBottomSheet"
        android:label="fragment_travel_request_bottom_sheet"
        tools:layout="@layout/fragment_travel_request_bottom_sheet"/>

    <fragment
        android:id="@+id/navi_createform_bottomsheet"
        android:name="com.worksy.hr.art.views.fragments.claimRequest.CreateFormBottomSheet"
        android:label="fragment_create_form_bottom_sheet"
        tools:layout="@layout/fragment_create_form_bottom_sheet"/>

    <fragment
        android:id="@+id/navi_travel_request_detailpage"
        android:name="com.worksy.hr.art.views.fragments.travelRequest.TravelRequestDetailPage"
        android:label="fragment_travel_request_detail_page"
        tools:layout="@layout/fragment_travel_request_detail_page"/>
    <fragment
        android:id="@+id/navi_lets_get_started"
        android:name="com.worksy.hr.art.views.fragments.onboard.LetsGetStartedFragment"
        android:label="fragment_lets_get_started"
        tools:layout="@layout/fragment_lets_get_started">
        <action
            android:id="@+id/action_lets_get_started_to_login_qrcode"
            app:destination="@+id/navi_qr_code_login"/>

    </fragment>
    <fragment
        android:id="@+id/navi_qr_code_login"
        android:name="com.worksy.hr.art.views.fragments.login.LoginQRFragment"
        android:label="fragment_login_q_r"
        tools:layout="@layout/fragment_login_q_r">
        <action android:id="@+id/action_navi_login_qr_to_home_permission_setup"
            app:destination="@+id/navi_home_permission_setup"/>
    </fragment>
    <fragment
        android:id="@+id/navi_loginwith_companyId"
        android:name="com.worksy.hr.art.views.fragments.login.LoginByCompanyID"
        android:label="fragment_login_by_company_i_d"
        tools:layout="@layout/fragment_login_by_company_i_d">

        <action android:id="@+id/action_navi_companyloginid_to_register"
            app:destination="@+id/navi_register"/>

        <action android:id="@+id/action_navi_companyloginid_to_forgotpassword"
            app:destination="@+id/navi_resetpassword"/>

        <action android:id="@+id/action_navi_companyloginid_to_homepermissionsetup"
            app:destination="@+id/navi_home_permission_setup"/>

    </fragment>

    <fragment
        android:id="@+id/navi_loginwith_mobile_number"
        android:name="com.worksy.hr.art.views.fragments.login.LoginByMobileNumber"
        android:label="fragment_login_by_mobile_number"
        tools:layout="@layout/fragment_login_by_mobile_number">
        <action android:id="@+id/action_navi_mobilenumber_to_register"
            app:destination="@+id/navi_register"/>
        <action android:id="@+id/action_navi_mobilenumber_to_forgotpassword"
            app:destination="@+id/navi_resetpassword"/>
    </fragment>

    <fragment
        android:id="@+id/navi_loginwith_email"
        android:name="com.worksy.hr.art.views.fragments.login.LoginWithEmail"
        android:label="fragment_login_with_email"
        tools:layout="@layout/fragment_login_with_email">
        <action android:id="@+id/action_navi_mobilenumber_to_register"
            app:destination="@+id/navi_register"/>
        <action android:id="@+id/action_navi_mobilenumber_to_forgotpassword"
            app:destination="@+id/navi_resetpassword"/>
    </fragment>
    <fragment
        android:id="@+id/navi_register"
        android:name="com.worksy.hr.art.views.fragments.register.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register">

    </fragment>
    <fragment
        android:id="@+id/navi_resetpassword"
        android:name="com.worksy.hr.art.views.fragments.resetpassword.ResetPasswordFragment"
        android:label="fragment_reset_password"
        tools:layout="@layout/fragment_reset_password">
        <action
            android:id="@+id/action_resetpassword_to_otpverify"
            app:destination="@id/navi_otpverify" />
    </fragment>
    <fragment
        android:id="@+id/navi_otpverify"
        android:name="com.worksy.hr.art.views.fragments.resetpassword.OtpVerifyFragment"
        android:label="fragment_otp_verify"
        tools:layout="@layout/fragment_otp_verify">
        <action
            android:id="@+id/action_otpverify_to_create_new_password"
            app:destination="@id/navi_create_new_password" />
    </fragment>
    <fragment
        android:id="@+id/navi_create_new_password"
        android:name="com.worksy.hr.art.views.fragments.resetpassword.CreateNewPassword"
        android:label="fragment_create_new_password"
        tools:layout="@layout/fragment_create_new_password">
        <action
            android:id="@+id/action_create_new_password_to_otpverify"
            app:destination="@id/navi_otpverify" />
    </fragment>
    <fragment
        android:id="@+id/navi_home_permission_setup"
        android:name="com.worksy.hr.art.views.fragments.permissionsetup.HomePermissionSetupFragment"
        android:label="fragment_home_permission_setup"
        tools:layout="@layout/fragment_home_permission_setup">
        <action
            android:id="@+id/action_home_permission_to_permission_setup"
            app:destination="@id/navi_permission_setup" />
        <action
            android:id="@+id/action_home_permission_to_clock_setup"
            app:destination="@id/navi_clock_setup" />
        <action
            android:id="@+id/action_home_permission_to_successful_setup"
            app:destination="@id/navi_successful_setup" />
    </fragment>
    <fragment
        android:id="@+id/navi_permission_setup"
        android:name="com.worksy.hr.art.views.fragments.permissionsetup.PermissionSetUpFragment"
        android:label="fragment_permission_set_up"
        tools:layout="@layout/fragment_permission_set_up">
    </fragment>
    <fragment
        android:id="@+id/navi_clock_setup"
        android:name="com.worksy.hr.art.views.fragments.permissionsetup.ClockDeviceSetupFragment"
        android:label="fragment_clock_device_setup"
        tools:layout="@layout/fragment_clock_device_setup">
    </fragment>
    <fragment
        android:id="@+id/navi_successful_setup"
        android:name="com.worksy.hr.art.views.fragments.permissionsetup.SuccessfulSetupFragment"
        android:label="fragment_successful_setup"
        tools:layout="@layout/fragment_successful_setup">
        <action
            android:id="@+id/action_successful_setup_to_company_selection"
            app:destination="@id/navi_company_selection" />
    </fragment>
    <fragment
        android:id="@+id/navi_company_selection"
        android:name="com.worksy.hr.art.views.fragments.companyselection.CompanySelectionFragment"
        android:label="fragment_company_selection"
        tools:layout="@layout/fragment_company_selection">
    </fragment>
</navigation>